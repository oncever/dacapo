benchmark avrora
  class org.dacapo.harness.Avrora
  thread-model single
  jar "avrora-cvs-20090612.jar";

size small args "-seconds=30",
		"-platform=mica2",
        "-simulation=sensor-network",
        "-nodecount=3,3",
		"-stagger-start=1000000",
		"test/tinyos/CntToRfm.elf",
		"test/tinyos/RfmToLeds.elf"
