         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

size defaultmem args "-driver","org.hsqldb.jdbcDriver",
                  "-url","jdbc:hsqldb:mem:hsqldb",
		  "-user","sa","-init",
		  "-clients","20",
		  "-tpc","2",
		  "-tps","2","-v"
  output "$stdout" digest 0x6f080549e69a5a9aba56ea6be2478ae0aba8ecac,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

size large args "-driver","org.hsqldb.jdbcDriver",
                "-url","jdbc:hsqldb:scratch/hsqldb",
		"-user","sa","-init",
		"-clients","20",
		"-tpc","5",
		"-tps","15","-v"
  output "$stdout" digest 0x435383b07a2da1e6d44b1ee195322c564bbe00c7,
