benchmark hsqldb 
  class dacapo.hsqldb.HsqldbHarness;

size small args "-driver","org.hsqldb.jdbcDriver",
                "-url","jdbc:hsqldb:scratch/hsqldb",
		"-user","sa","-init",
		"-clients","10",
		"-tpc", "5",
		"-tps","4","-v"
  output "$stdout" digest 0xe5254c965420a131f264be5adbdbf5f55dbb5b98,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

size default args "-driver","org.hsqldb.jdbcDriver",
                  "-url","jdbc:hsqldb:scratch/hsqldb",
		  "-user","sa","-init",
		  "-clients","40",
		  "-tpc","20",
		  "-tps","32"
  output "$stdout" digest 0xd742ecfa28f1c059ad9246182a0c8cccf762ec65,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

size large args "-driver","org.hsqldb.jdbcDriver",
                "-url","jdbc:hsqldb:scratch/hsqldb",
		"-user","sa","-init",
		"-clients","40",
		"-tpc","20",
		"-tps","64"
  output "$stdout" digest 0xfbecc98be3c3cfa4589532d0bd6b1a01b933b538,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;


