benchmark hsqldb 
  class dacapo.hsqldb.HsqldbHarness;

size small args "-driver","org.hsqldb.jdbcDriver",
                "-url","jdbc:hsqldb:scratch/hsqldb",
		"-user","sa","-init",
		"-clients","20",
		"-tpc", "5",
		"-tps","8","-v"
  output "$stdout" digest 0x9f1b83df6d2e9918a1ae17bf6c0e09ce88c1407d,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

size default args "-driver","org.hsqldb.jdbcDriver",
                  "-url","jdbc:hsqldb:scratch/hsqldb",
		  "-user","sa","-init",
		  "-clients","40",
		  "-tpc","20",
		  "-tps","32"
  output "$stdout" digest 0xba418e45b6ba5a6afbe57025007bf14a7cf23b1d,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

size large args "-driver","org.hsqldb.jdbcDriver",
                "-url","jdbc:hsqldb:scratch/hsqldb",
		"-user","sa","-init",
		"-clients","40",
		"-tpc","20",
		"-tps","64"
  output "$stdout" digest 0xc8cba6b00e65ca4edac74579f2d3ecb2af280a8d,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;


