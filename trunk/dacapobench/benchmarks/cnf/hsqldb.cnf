benchmark hsqldb 
  class dacapo.hsqldb.HsqldbHarness;

size small args "-driver","org.hsqldb.jdbcDriver",
                "-url","jdbc:hsqldb:scratch/hsqldb",
		"-user","sa","-init",
		"-clients","1",
		"-tpc", "1",
		"-tps","1","-v"
  output "$stdout" digest 0x1127e66a443f3b2d92110cbb2f4542bc51a6c0d8,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;
size smallmem args "-driver","org.hsqldb.jdbcDriver",
                "-url","jdbc:hsqldb:mem:hsqldb",
		"-user","sa","-init",
		"-clients","1",
		"-tpc", "1",
		"-tps","1","-v"
  output "$stdout" digest 0xb889564808ee5e2ccdd52e9af1d631a22c856d77,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

