benchmark xalan
  class dacapo.xalan.XalanHarness;

size small args "2","1"
  output "$stderr" digest 0x71853c6197a6a7f222db0f1978c7cb232b87c5ee;

size default args "8","2"
  output "$stderr" digest 0xf080446304324098b74b9addc0a16960d33e1b84;

size large args "16","2"
