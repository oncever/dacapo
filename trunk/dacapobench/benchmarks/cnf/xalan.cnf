benchmark xalan
  class dacapo.xalan.XalanHarness;

size small args "2","1"
  output "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

size default args "8","2"
  output "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

size large args "16","2"
  output "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709;

description
  short	"An XSLT processor for transforming XML documents",
  long	"Xalan transforms an XML document (either a test case or the works of Shakespeare) and transforms the document into html",
  copyright	"Copyright (C) 1999-2003 The Apache Software Foundation",
  url	"http://xml.apache.org/xalan-j/",
  version	"2.4.1";
