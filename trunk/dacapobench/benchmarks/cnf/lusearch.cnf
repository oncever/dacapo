benchmark lusearch
  class dacapo.lucene.LuceneSearch;

size small args "-index","lusearch/index-default",
                "-queries","lusearch/queries-small.txt"
  output "$stdout" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709,
	 "lucene-search.out" digest 0x93936a7c5bd89df22ba2d3a57d1b3c5132fcdbde;

size default args "-index","lusearch/index-default",
                "-queries","lusearch/queries-default.txt"
  output "$stdout" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709,
         "$stderr" digest 0xda39a3ee5e6b4b0d3255bfef95601890afd80709,
	 "lucene-search.out" digest 0x1ad7ca372ff9193d40bbbb6a44ffb0b9701b0e4e;

size large args "-index","lusearch/index-default",
                "-queries","lusearch/queries-large.txt"
